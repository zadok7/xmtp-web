{
  "name": "showmehow-eth-messenger",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn typecheck && vite build",
    "clean:dist": "rm -rf dist",
    "clean": "rm -rf .turbo && rm -rf node_modules && yarn clean:dist",
    "lint": "eslint . --ignore-path ../../.gitignore",
