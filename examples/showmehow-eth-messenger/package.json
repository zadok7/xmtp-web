    "@xmtp/react-components": "workspace:*",
    "@xmtp/react-sdk": "workspace:*",
    "ethereum-ens": "^0.8.0",
    "ethers": "5.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^0.12.18"
  },
  "devDependencies": {
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "@xmtp/tsconfig": "workspace:*",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
